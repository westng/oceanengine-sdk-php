{
  "name": "westng/oceanengine-sdk-php",
  "version": "4.0.0",
  "description": "巨量引擎开放平台PHP语言SDK",
  "keywords": [
    "巨量引擎",
    "巨量千川",
    "巨量本地推",
    "巨量星图",
    "sdk"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "authors": [
    {
      "name": "westng",
      "email": "457395070@qq.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "OceanEngineSDK\\": "src/Core/Profile/",
      "Oauth\\": "src/Oauth/",
      "Account\\": "src/Api/Account/",
      "DataReports\\": "src/Api/DataReports/",
      "Tools\\": "src/Api/Tools",
      "Materials\\": "src/Api/Materials",
      "Core\\": "src/Core/",
      "Api\\": "src/Api/",
      "EnterpriseAccount\\": "src/Api/EnterpriseAccount/",
      "JuLiangAds\\": "src/Api/JuLiangAds/",
      "JuLiangLocalPush\\": "src/Api/JuLiangLocalPush/",
      "JuLiangQianChuan\\": "src/Api/JuLiangQianChuan/",
      "JuLiangStarMap\\": "src/Api/JuLiangStarMap/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-fix": "php-cs-fixer fix",
    "test:unit": "php -d zend.assertions=1 -d assert.exception=1 vendor/bin/phpunit --testsuite unit",
    "test:module": "php -d zend.assertions=1 -d assert.exception=1 vendor/bin/phpunit --testsuite module",
    "test:integration": "php -d zend.assertions=1 -d assert.exception=1 vendor/bin/phpunit --testsuite integration",
    "test:integration:file": "php -d zend.assertions=1 -d assert.exception=1 vendor/bin/phpunit",
    "test": [
      "@test:unit",
      "@test:module"
    ],
    "test:all": [
      "@test:unit",
      "@test:module",
      "@test:integration"
    ]
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.75",
    "phpunit/phpunit": "^9.6"
  }
}
